<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite 
	PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
	"http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/$</from>
		<to type="forward">/index.mvn</to>
	</rule>
	<rule>
		<from>^/winner$</from>
		<to type="forward">/winner.mvn</to>
	</rule>
	<rule>
		<from>^/form$</from>
		<to type="forward">/form.mvn</to>
	</rule>
	<rule>
		<from>^/tnc$</from>
		<to type="forward">/tnc.mvn</to>
	</rule>
	<rule>
		<from>^/thankyou/(.*)$</from>
		<to type="forward">/thank_you.mvn?id=$1</to>
	</rule>
<!-- 
	<rule>
		<from>^/login$</from>
		<to type="forward">/login.mvn</to>
	</rule>
	<rule>
		<from>^/login/fail$</from>
		<to type="forward">/login.mvn?fail=1</to>
	</rule>
 -->
</urlrewrite>